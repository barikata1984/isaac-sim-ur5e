# Python 3.11 Unified Environment Variables
# Copy this to .env when using the Python 3.11 unified environment

# ROS 2 Configuration
ROS_DISTRO=jazzy
ROS_PYTHON_VERSION=3

# Python path for ROS 2 Python 3.11 packages
PYTHONPATH=/opt/ros311/lib/python3.11/site-packages

# Library path
LD_LIBRARY_PATH=/opt/ros311/lib:/opt/ros/jazzy/lib
